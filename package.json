    "jest": {
        "transform": {
            "^.+\\.js$": "babel-jest"
        },
        "verbose": true,
        "setupFilesAfterEnv": [
            "<rootDir>/src/tests/setupTest.js"
        ]
    },
    "scripts": {
        "test": "set NODE_ENV=development&& jest --coverage",
        "start": "set NODE_ENV=production&& node ./src/server.js",
